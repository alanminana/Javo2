@model IEnumerable<dynamic>
@{
    ViewBag.Title = "Ranking de Ventas";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@ViewBag.Title</h2>
<table class="table table-dark table-striped">
    <thead>
        <tr>
            <th>Cliente</th>
            <th>Total Vendido</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Cliente</td>
                <td>@item.SumaTotal</td>
            </tr>
        }
    </tbody>
</table>
